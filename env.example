# Solana RPC URL (use a high-performance RPC for production)
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Wallet private key (base58 encoded)
WALLET_PRIVATE_KEY=your_private_key_here

# Optional: Database connection string (if using Prisma)
DATABASE_URL=postgresql://username:password@localhost:5432/pumpfun?schema=public

# Vanity Address Configuration
VANITY_SUFFIX=pump
VANITY_POOL_MIN=100
VANITY_POOL_MAX=250
VANITY_REFILL_BATCH=32
VANITY_AES256_KEY=your_64_character_hex_key_here
VANITY_RES_TTL_MS=300000
VANITY_WORKERS=3
VANITY_POOL_FILE=./vanity-keypairs.json
